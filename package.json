{
  "name": "tooey",
  "version": "0.1.0",
  "description": "Token-efficient UI library for LLMs",
  "main": "dist/tooey.js",
  "module": "dist/tooey.esm.js",
  "types": "dist/tooey.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js --watch",
    "serve": "npx serve .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "typecheck": "tsc --noEmit",
    "count-tokens": "node count-tokens.js"
  },
  "keywords": [
    "ui",
    "llm",
    "token-efficient",
    "declarative"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "esbuild": "^0.27.2",
    "eslint": "^9.0.0",
    "gpt-tokenizer": "^3.4.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.17"
  }
}
