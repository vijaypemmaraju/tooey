{
  "name": "@tooey/components",
  "version": "0.1.0",
  "description": "shadcn-inspired component library for @tooey/ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tooey",
    "ui",
    "components",
    "shadcn",
    "llm",
    "token-efficient"
  ],
  "author": "Vijay Pemmaraju",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijaypemmaraju/tooey.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/vijaypemmaraju/tooey/issues"
  },
  "homepage": "https://github.com/vijaypemmaraju/tooey/tree/main/packages/components#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tooey/ui": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^3.0.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.0.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  }
}
